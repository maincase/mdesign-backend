{
  "name": "modern-design-backend",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env DEBUG=mdesign:* NODE_ENV=development TS_NODE_FILES=true nodemon -e js,mjs,json,ts,mts --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm ./bin/www'",
    "build": "cross-env NODE_ENV=production rimraf build && tsc && copyfiles ./bin/www ./build",
    "start:production": "cross-env DEBUG=mdesign:* NODE_ENV=production node --experimental-specifier-resolution=node --loader ts-node/esm ./build/bin/www",
    "eslint": "eslint --ext .ts,.tsx .",
    "eslint:check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@google-cloud/vision": "^4.1.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "google-auth-library": "^9.6.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "mongoose": "^7.6.8",
    "multer": "1.4.5-lts.1",
    "passport": "^0.7.0",
    "replicate": "^0.27.1",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^4.1.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
