{
  "name": "modern-design-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "./node_modules/.bin/cross-env DEBUG=mdesign:* NODE_ENV=development TS_NODE_FILES=true nodemon -e js,mjs,json,ts,mts --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm ./bin/www.ts'",
    "build": "./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/rimraf build && ./node_modules/.bin/tsc && ./node_modules/.bin/copyfiles ./bin/www ./build",
    "start:production": "./node_modules/.bin/cross-env DEBUG=mdesign:* NODE_ENV=production node --experimental-specifier-resolution=node ./build/bin/www",
    "eslint": "eslint --ext .ts,.tsx .",
    "eslint:check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.11.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "google-auth-library": "^8.9.0",
    "got": "^11.8.6",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "mongodb": "^5.7.0",
    "mongoose": "^7.3.3",
    "multer": "^1.4.2",
    "passport": "^0.6.0",
    "remeda": "^1.23.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.8",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.195",
    "@types/mongodb": "^4.0.6",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^4.1.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
