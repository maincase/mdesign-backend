{
  "name": "modern-design-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "./node_modules/.bin/cross-env NODE_ENV=development TS_NODE_FILES=true ts-node-dev --clear --rs ./bin/www",
    "build": "./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/rimraf build && ./node_modules/.bin/tsc && ./node_modules/.bin/copyfiles ./bin/www ./build",
    "start:production": "./node_modules/.bin/cross-env NODE_ENV=production node ./build/bin/www",
    "eslint": "eslint --ext .ts,.tsx .",
    "eslint:check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.10.1",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "got": "^12.6.0",
    "http-errors": "~2.0.0",
    "jsdom": "^22.0.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongodb": "^5.5.0",
    "mongoose": "^7.2.1",
    "multer": "^1.4.2",
    "multer-google-storage": "^1.3.0",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0"
